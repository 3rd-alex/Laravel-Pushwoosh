{
    "name": "hoy/pushwoosh",
    "description": "A Pushwoosh bridge for Laravel",
    "license": "MIT",
    "keywords": ["pushwoosh", "push", "notification", "hoy", "laravel"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "vincent@hoy.se"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/contracts": "5.1.* || 5.2.*",
        "illuminate/support": "5.1.* || 5.2.*",
        "graham-campbell/manager": "^2.3",
        "gomoob/php-pushwoosh": "^1.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Hoy\\Pushwoosh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hoy\\Tests\\Pushwoosh\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
